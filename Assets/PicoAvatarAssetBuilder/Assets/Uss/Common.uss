.middle-ellipsis__button-label {
    background-color: rgb(20, 20, 20);
    border-right-width: 0;
    border-left-width: 0;
    border-top-left-radius: 0;
    border-bottom-left-radius: 0;
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
    border-top-width: 0.5px;
    border-bottom-width: 0.5px;
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
    padding-left: 0;
    padding-right: 0;
    padding-top: 10px;
    padding-bottom: 10px;
    overflow: hidden;
    text-overflow: ellipsis;
    width: 181px;
    justify-content: center;
    align-items: center;
    left: 0;
    word-spacing: 0;
}

.middle-ellipsis__label {
    width: 113px;
    height: 20px;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    min-width: 113px;
    min-height: 20px;
    flex-shrink: 0;
    left: 25px;
    flex-wrap: nowrap;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    -unity-text-align: middle-center;
}

