    background-color: rgb(20, 20, 20);
    padding-left: 14px;
    padding-right: 16px;
    padding-top: 14px;
    padding-bottom: 16px;
    max-width: initial;
    max-height: initial;
    min-height: auto;
    min-width: auto;
    display: flex;
    font-size: 16px;
    -unity-font-style: bold;
